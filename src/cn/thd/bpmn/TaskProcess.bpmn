  <process id="taskProcess" name="Task Process" isExecutable="true">
    <startEvent id="start" name="开始"></startEvent>
    <userTask id="regist" name="任务登记及派工" activiti:candidateUsers="${assigner}" activiti:formKey="seProcess/seProcess!taskProcessAssign.do"></userTask>
    <userTask id="perform" name="执行" activiti:candidateUsers="${performer}" activiti:formKey="seProcess/seProcess!taskProcessPerform.do"></userTask>
    <userTask id="audit" name="审核" activiti:candidateUsers="${auditer}" activiti:formKey="seProcess/seProcess!taskProcessAudit.do"></userTask>
    <exclusiveGateway id="decide" name="判定"></exclusiveGateway>
    <endEvent id="end" name="结束"></endEvent>
    <sequenceFlow id="toEnd" name="到结束" sourceRef="decide" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditStatus==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="toPerform" name="到执行" sourceRef="regist" targetRef="perform"></sequenceFlow>
    <sequenceFlow id="toRegist" name="到任务指派" sourceRef="start" targetRef="regist"></sequenceFlow>
    <sequenceFlow id="toDecide" name="到判定" sourceRef="audit" targetRef="decide"></sequenceFlow>
    <sequenceFlow id="toAudit" name="到审核" sourceRef="perform" targetRef="audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${performStatus==1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="toUnPassPerform" name="不通过" sourceRef="decide" targetRef="perform">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditStatus==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sendBack" name="退回派工" sourceRef="perform" targetRef="regist">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${performStatus==0}]]></conditionExpression>
    </sequenceFlow>
  </process>
